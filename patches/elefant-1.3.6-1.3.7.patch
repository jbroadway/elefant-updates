+			return $this->_send_and_receive ($url . $options_string, 'GET');
+		}
+
+		// URL/plural/id.xml?someparam=1
+		return $this->_send_and_receive ($this->site . $this->element_name_plural . '/' . $id . '.xml' . $options_string, 'GET');
+	}
