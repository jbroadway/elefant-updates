+				if (!this.opts.properties.show && !this.opts.properties.classname)
+				{
+					return;
+				}
+
+				this.properties.labelClass = $('<span />').attr('id', 'redactor-properties-label-class-' + this.uuid).attr('title', 'class').hide();
