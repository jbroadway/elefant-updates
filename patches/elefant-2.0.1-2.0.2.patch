+				 return String()
+				 + '<div class="modal-section" id="redactor-modal-properties">'
+				 	+ '<section>'
+				 		+ '<label id="modal-properties-id-label">Id</label>'
+				 		+ '<input type="text" id="modal-properties-id" />'
+				 	+ '</section>'
+				 	+ '<section>'
+					 	+ '<label id="modal-properties-class-label">Class</label>'
