+	samp,
+	kbd {
+		padding: 2px 4px 1px 4px;
+		display: inline-block;
+		line-height: 1;
+		border-radius: 3px;
+	}
+	code {
+		background: $code-background-color;
+	}
+	pre code {
+		font-size: 100%;
+		border: none;
+		padding: 0;
+		background: none;
+		line-height: $base-line;
+	}
+	var {
+		color: rgba(0, 0, 0, .5);
+	}
+	samp {
+		background: #46a9fc;
