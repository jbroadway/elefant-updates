+			},
+			removeAlign: function()
+			{
+    		    this.block.removeClass('text-center');
+    		    this.block.removeClass('text-right');
+    		    this.block.removeClass('text-justify');
+			},
+			setLeft: function()
+			{
+				this.buffer.set();
+				this.alignment.removeAlign();
