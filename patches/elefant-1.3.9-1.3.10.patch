+		
+		foreach ($posts as $i => $post) {
+			$url = 'blog/post/' . $post->id . '/' . \URLify::filter ($post->title);
+			if (! \Search::add (
+				$url,
+				array (
+					'title' => $post->title,
+					'text' => $post->body,
+					'url' => '/' . $url
+				)
+			)) {
+				return array (false, $i);
+			}
+		}
